{% extends 'base.html.twig' %}{% block title %}Hello {{ controller_name }}!{% endblock %}{% block body %}<br>    <div class="container">        <br>        <ul class="nav nav-tabs" id="myTab" role="tablist">            <li class="nav-item">                <a class="nav-link active" id="home-tab" data-toggle="tab" href="#home" role="tab" aria-controls="home" aria-selected="true">Home</a>            </li>            <li class="nav-item">                <a class="nav-link" id="profile-tab" data-toggle="tab" href="#profile" role="tab" aria-controls="profile" aria-selected="false">Profile</a>            </li>        </ul>        <div class="tab-content" id="myTabContent">            <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">                <table class="table">                    <tr>                        <th>#</th>                        <th>Familia</th>                        <th>Artículo</th>                        <th>Marca</th>                        <th>Modelo</th>                        <th>Cantidad</th>                    </tr>                {% for dat in orden %}                    <tr>                        <td>{{ dat.id }}</td>                    </tr>                {% endfor %}                </table>            </div>            <div class="tab-pane fade" id="profile" role="tabpanel" aria-labelledby="profile-tab">            </div>        </div>        <div class="alert alert-info" role="alert">            Cabecera        </div>        <div >            {{ cabe.destino }}        </div>    <div class="alert alert-info" role="alert">        Lista de los artículos a solicitar    </div>    {{ form_start(formulario) }}    <table class="table">        <tr>            <th>#</th>            <th>Familia</th>            <th>Artículo</th>            <th>Marca</th>            <th>Modelo</th>            <th>Cantidad</th>        </tr>        {% for datos in artList %}            <tr>                <td>{{ datos.id }}</td>                <td>{{ datos.familia }}</td>                <td>{{ datos.articulo }}</td>                <td>{{ datos.marca }}</td>                <td>{{ datos.modelo }}</td>                <td style="width:30px;">{{ form_widget(formulario[datos.id], {'attr': {'value': '0'}}) }}</td>            </tr>        {% endfor %}    </table>    {{ form_end(formulario) }}    </div>{% endblock %}
{% extends 'base.html.twig' %}{% block title %}Lista {% endblock %}{% block body %}    <style>        .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }        .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }    </style>    <div class="table table-sm">        <h1>Lista de Artículos</h1>        <table class="table">            <tr>                <th>#</th>                <th>Fecha</th>                <th>Nro. Serie</th>                <th>Cod. Barra</th>                <th>Familia</th>                <th>Artículo</th>                <th>Marca</th>                <th>Modelo</th>                <th>.</th>                <th>.</th>            </tr>            {% for art in art %}                <tr>                    <th>{{ art.id }}</th>                    <td>{{ art.nroSerie }}</td>                    <td>{{ art.nroSerie }}</td>                    <td>{{ art.codBarra }}</td>                    <td>{{ art.familia }}</td>                    <td>{{ art.articulo }}</td>                    <td>{{ art.marca }}</td>                    <td>{{ art.modelo }}</td>                    <td>{{ art.detalle }}</td>                    <td >borrar</td>                    <td>editar</td>                </tr>            {% endfor %}        </table>    </div>{% endblock %}
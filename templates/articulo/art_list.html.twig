{% extends 'base.html.twig' %}{% block title %}Lista {% endblock %}{% block body %}    <div class="table table-sm">        <h1>Lista de Artículos</h1>        <table class="table">            <tr>                <th>#</th>                <th>Familia</th>                <th>Artículo</th>                <th>Marca</th>                <th>Modelo</th>                <th>.</th>                <th>.</th>            </tr>            {% for art in art %}                <tr>                    <th>{{ art.id }}</th>                    <td>{{ art.familia }}</td>                    <td>{{ art.articulo }}</td>                    <td>{{ art.marca }}</td>                    <td>{{ art.modelo }}</td>                    <td>{{ art.detalle }}</td>                    <td >borrar</td>                    <td>editar</td>                </tr>            {% endfor %}        </table>    </div>{% endblock %}
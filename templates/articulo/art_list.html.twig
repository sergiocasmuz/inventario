{% extends 'base.html.twig' %}{% block title %}Lista {% endblock %}{% block body %}    <style>        .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }        .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }    </style>    <div class="example-wrapper">        <h1>Lista de Art√≠culos</h1>        <table>            {% for art in art %}                <tr>                    <td>{{ art.id }}</td>                    <td>{{ art.nroSerie }}</td>                    <td>{{ art.codBarra }}</td>                    <td>{{ art.familia }}</td>                    <td>{{ art.articulo }}</td>                    <td>{{ art.marca }}</td>                    <td>{{ art.modelo }}</td>                    <td>{{ art.detalle }}</td>                    <td >borrar</td>                    <td>editar</td>                </tr>            {% endfor %}        </table>    </div>{% endblock %}